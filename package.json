{
  "name": "camper-stockcharts",
  "version": "1.0.0",
  "description": "Chart the Stock Market",
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "node server/index.js",
    "start-server": "nodemon --use-strict server/index.js",
    "lint": "eslint .",
    "bundle": "webpack --watch",
    "heroku-build": "webpack",
    "heroku-postbuild": "cd client/ && npm install && cd .. && npm install && npm run heroku-build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roxroy/camper-stockcharts.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "node"
  ],
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "mongoose": "^4.10.2",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^0.2.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}